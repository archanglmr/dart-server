{
  "name": "dart-server",
  "version": "0.0.0",
  "description": "Framework for dartboard games",
  "private": true,
  "scripts": {
    "start": "node --harmony-tailcalls ./bin/www",
    "restart": "npm run build; npm start",
    "build": "webpack",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "./node_modules/mocha/bin/mocha test/unit/*.test.js",
    "test-integration": "./node_modules/mocha/bin/mocha test/integration/*.test.js",
    "postinstall": "npm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archanglmr/dart-server.git"
  },
  "keywords": [
    "darts",
    "game",
    "server"
  ],
  "author": "Matt Rossetta",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/archanglmr/dart-server/issues"
  },
  "homepage": "https://github.com/archanglmr/dart-server#readme",
  "engines": {
    "node": ">=6.2.1",
    "npm": ">=3.8.9"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-handlebars": "^3.0.0",
    "morgan": "~1.6.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "sequelize": "^3.23.3",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.6",
    "sqlite3": "^3.1.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.3",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^2.5.3",
    "node-reset-scss": "^1.0.1",
    "node-sass": "^3.7.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
